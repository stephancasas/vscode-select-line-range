{
    "name": "select-line-range",
    "displayName": "Select Line Range",
    "description": "Select a range of lines, or select from the current line to the Nth line.",
    "author": "Stephan Casas <stephancasas@icloud.com>",
    "publisher": "StephanCasas",
    "repository": {
        "type": "git",
        "url": "https://github.com/stephancasas/vscode-select-line-range"
    },
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.64.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:select-line-range.selectLineRange"
    ],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "select-line-range.selectLineRange",
                "title": "Select Line Range"
            }
        ],
        "menus": {
            "commandPalette": [
                {
                    "command": "select-line-range.selectLineRange"
                }
            ]
        }
    },
    "devDependencies": {
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.0.0",
        "@types/node": "14.x",
        "@types/vscode": "^1.64.0",
        "@vscode/test-electron": "^2.0.3",
        "eslint": "^8.6.0",
        "glob": "^7.2.0",
        "mocha": "^9.1.3",
        "typescript": "^4.5.4"
    }
}